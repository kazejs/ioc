{
  "name": "@kazejs/ioc",
  "description": "Biblioteca de Invers√£o de Controle (IoC) para TypeScript e JavaScript, para Deno e Node.js.",
  "version": "1.0.2",
  "license": "MIT",
  "exports": {
    ".": "./mod.ts",
    "./types": "./types.ts",
    "./hono": "./hono/mod.ts"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1.0.14"
  },
  "exclude": [
    "dist/",
    ".tmp/",
    ".coverage/"
  ],
  "coverageThreshold": {
    "branches": 40,
    "lines": 80
  },
  "lint": {
    "exclude": [
      "dist/",
      ".coverage/",
      "scripts/mods/"
    ]
  },
  "publish": {
    "include": [
      "src",
      "mod.ts",
      "types.ts",
      "LICENSE",
      "README.md"
    ],
    "exclude": [
      "tests",
      "scripts",
      ".tmp",
      "node_modules",
      "scripts/mods"
    ]
  }
}
