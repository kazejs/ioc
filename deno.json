{
  "name": "@kazejs/ioc",
  "description": "Biblioteca de Invers√£o de Controle (IoC) para TypeScript e JavaScript, para Deno e Node.js.",
  "version": "0.0.1",
  "license": "MIT",
  "exports": {
    ".": "./mod.ts",
    "./types": "./types.ts",
    "./hono": "./src/hono/mod.ts"
  },
  "tasks": {
    "test:npm": "deno run -A scripts/build_npm.ts --test",
    "build:npm": "deno run -A scripts/build_npm.ts",
    "publish:npm": "npm publish dist/ --access public",
    "publish:deno": "deno publish",
    "test": "deno test",
    "test:cov": "deno task test --coverage=.coverage && deno coverage --html .coverage",
    "test:cov-check": "deno coverage --lcov --output=.coverage/coverage.lcov .coverage && deno coverage .coverage && deno --allow-read scripts/check_coverage.ts"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1.0.14",
    "hono": "npm:hono@^4.9.9"
  },
  "exclude": [
    "dist/",
    ".tmp/",
    ".coverage/"
  ],
  "coverageThreshold": {
    "branches": 40,
    "lines": 80
  },
  "lint": {
    "exclude": [
      "dist/",
      ".coverage/"
    ]
  },
  "publish": {
    "include": [
      "src",
      "mod.ts",
      "types.ts",
      "hono.ts",
      "LICENSE",
      "README.md"
    ],
    "exclude": [
      "tests",
      "scripts",
      ".tmp",
      "node_modules"
    ]
  }
}
